---
import "../assets/styles/font-awesome.min.css";
import "../assets/styles/bootstrap.min.css";
import "../assets/styles/style.css";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>{title}</title>
    <!-- Search Engine -->
    <meta
      name="description"
      content="A team of software experts with technological leadership and a consulting partner for the management and governance of complex software architectures"
    />
    <meta
      name="keywords"
      content="scalable software platforms, technical partners, Elixir, Erlang, RabbitMQ, React, React Native, CQRS, es, event sourcing, nodejs, javascript, typescript, microservices, ddd, tdd, domain driven design"
    />
    <!-- Schema.org for Google -->
    <meta
      itemprop="name"
      content="coders51 - We design, build and evolve scalable software platforms"
    />
    <meta
      itemprop="description"
      content="A team of software experts with technological leadership and a consulting partner for the management and governance of complex software architectures"
    />
    <!-- Twitter -->
    <meta name="twitter:card" content="summary" />
    <meta
      name="twitter:title"
      content="coders51 - We design, build and evolve scalable software platforms"
    />
    <meta
      name="twitter:description"
      content="A team of software experts with technological leadership and a consulting partner for the management and governance of complex software architectures"
    />
    <meta name="twitter:site" content="@coders51" />
    <meta
      name="twitter:image:src"
      content="http://www.coders51.com/img/share2.jpg"
    />
    <!-- Open Graph general (Facebook, Pinterest & Google+) -->
    <meta
      name="og:title"
      content="coders51 - We design, build and evolve scalable software platforms"
    />
    <meta
      name="og:description"
      content="A team of software experts with technological leadership and a consulting partner for the management and governance of complex software architectures"
    />
    <meta name="og:image" content="http://www.coders51.com/img/share2.jpg" />
    <meta name="og:url" content="http://www.coders51.com/" />
    <meta name="og:site_name" content="coders51" />
    <meta name="fb:app_id" content="1028463787335394" />
    <meta name="og:type" content="website" />

    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <meta name="robots" content="index, follow" />
    <meta name="revisit-after" content="15" />
    <meta name="language" content="en-US" />
    <meta name="copyright" content="coders51" />
    <meta name="author" content="Michele CarrÃ¬ & Gianluca Padovani" />
    <meta name="reply-to" content="admin@coders51.com" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta http-equiv="cache-control" content="no-cache" />

    <!-- Favicon -->
    <link rel="shortcut icon" href="/img/favicon.ico" type="image/x-icon" />
    <link rel="icon" href="/img/favicon.ico" type="image/x-icon" />

    <!-- Google font -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:wght@300;400;600;700&display=swap"
      rel="stylesheet"
    />

    <!-- Font Awesome Icon -->
    <script
      src="https://kit.fontawesome.com/42531828f2.js"
      crossorigin="anonymous"></script>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script type="application/ld+json">
      {
        "@context": "http://schema.org",
        "@type": "Organization",
        "name": "coders51",
        "url": "https://coders51.com",
        "logo": "http://www.coders51.com/img/share2.jpg"
      }
    </script>
  </head>
  <body>
    <slot />

    <!-- Contact CTA -->
    <div id="contact-cta" class="section">
      <!-- Backgound Image -->
      <div
        class="bg-image bg-parallax overlay"
        style="background-image:url(/img/cta2-background.png)"
      >
      </div>
      <!-- Backgound Image -->

      <!-- container -->
      <div class="container">
        <!-- row -->
        <div class="row">
          <div class="col-md-8 col-md-offset-2 text-center">
            <h1 class="white-text">Contact Us</h1>
            <p class="lead white-text">
              If you're interested in what we do or you want to join our team
              feel free to reach us!
            </p>

            <a
              class="typeform-share main-button icon-button"
              href="https://coders51.typeform.com/to/BpB9pe"
              target="_blank"
              data-mode="popup">Contact Us</a
            >
            <script>
              // @ts-nocheck
              (function () {
                var qs,
                  js,
                  q,
                  s,
                  d = document,
                  gi = d.getElementById,
                  ce = d.createElement,
                  gt = d.getElementsByTagName,
                  id = "typef_orm_share",
                  b = "https://embed.typeform.com/";
                if (!gi.call(d, id)) {
                  js = ce.call(d, "script");
                  js.id = id;
                  js.src = b + "embed.js";
                  q = gt.call(d, "script")[0];
                  q.parentNode.insertBefore(js, q);
                }
              })();
            </script>
          </div>
        </div>
        <!-- /row -->
      </div>
      <!-- /container -->
    </div>
    <!-- /Contact CTA -->

    <!-- Footer -->
    <footer id="footer" class="section">
      <!-- container -->
      <div class="container">
        <!-- row -->
        <div class="row">
          <!-- footer logo -->
          <div class="col-md-6">
            <div class="footer-logo">
              <a class="logo" href="index.html">
                <img src="/img/logo.png" alt="logo" />
              </a>
            </div>
          </div>
          <!-- footer logo -->
        </div>
        <!-- /row -->

        <!-- row -->
        <div id="bottom-footer" class="row">
          <!-- social -->
          <div class="col-md-4 col-md-push-8">
            <ul class="footer-social">
              <li>
                <a
                  href="https://www.linkedin.com/company/coders51/"
                  target="new"
                  class="linkedin"
                  ><span class="sr-only">linkedin</span>
                  <i class="fa-brands fa-linkedin-in"></i></a
                >
              </li>
              <li>
                <a
                  href="https://twitter.com/coders51"
                  target="new"
                  class="x-twitter"
                >
                  <span class="sr-only">x-twitter</span>
                  <i class="fa-brands fa-x-twitter"></i></a
                >
              </li>
              <li>
                <a
                  href="https://www.facebook.com/coders51"
                  target="new"
                  class="facebook"
                >
                  <span class="sr-only">facebook</span>
                  <i class="fa-brands fa-facebook-f"></i></a
                >
              </li>
            </ul>
          </div>
          <!-- /social -->

          <!-- copyright -->
          <div class="col-md-8 col-md-pull-4">
            <div class="footer-copyright">
              <p>
                &copy; Copyright 2022. All Rights Reserved.
                <br />
                coders51 SRL - Viale Angelo Masini 12/14 - 40126 Bologna (BO) - Italy
                - VAT ID 03455241202
                <br />
              </p>
              <div class="privacy-container">
                <a
                  href="https://www.iubenda.com/privacy-policy/81936352"
                  class="iubenda-white no-brand iubenda-noiframe iubenda-embed iubenda-noiframe"
                  title="Privacy Policy ">Privacy Policy</a
                ><script type="text/javascript">
                  (function (w, d) {
                    var loader = function () {
                      var s = d.createElement("script"),
                        tag = d.getElementsByTagName("script")[0];
                      s.src = "https://cdn.iubenda.com/iubenda.js";
                      tag.parentNode.insertBefore(s, tag);
                    };
                    if (w.addEventListener) {
                      w.addEventListener("load", loader, false);
                    } else if (w.attachEvent) {
                      w.attachEvent("onload", loader);
                    } else {
                      w.onload = loader;
                    }
                  })(window, document);
                </script>
              </div>
            </div>
          </div>
          <!-- /copyright -->
        </div>
        <!-- row -->
      </div>
      <!-- /container -->

      <button onclick="topFunction()" id="scrollTopButton" title="Go to top"
        ><i class="fa-solid fa-chevron-up"></i></button
      >
    </footer>
    <!-- /Footer -->
  </body>

  <script type="text/javascript" src="/js/jquery.min.js"></script>
  <script type="text/javascript" src="/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="/js/main.js"></script>
  <script type="text/javascript">
    var _iub = _iub || [];
    _iub.csConfiguration = {
      ccpaAcknowledgeOnDisplay: true,
      ccpaApplies: true,
      consentOnContinuedBrowsing: false,
      enableCcpa: true,
      floatingPreferencesButtonDisplay: "bottom-left",
      invalidateConsentWithoutLog: true,
      lang: "en",
      perPurposeConsent: true,
      siteId: 2695371,
      whitelabel: false,
      cookiePolicyId: 81936352,
      banner: {
        acceptButtonDisplay: true,
        closeButtonDisplay: false,
        customizeButtonDisplay: true,
        explicitWithdrawal: true,
        listPurposes: true,
        position: "bottom",
        rejectButtonDisplay: true,
      },
    };
  </script>
  <script type="text/javascript" src="//cdn.iubenda.com/cs/ccpa/stub.js"
  ></script>
  <script
    type="text/javascript"
    src="//cdn.iubenda.com/cs/iubenda_cs.js"
    charset="UTF-8"
    async></script>

  <script>
    // @ts-nocheck
    (function (i, s, o, g, r, a, m) {
      i["GoogleAnalyticsObject"] = r;
      (i[r] =
        i[r] ||
        function () {
          (i[r].q = i[r].q || []).push(arguments);
        }),
        (i[r].l = 1 * new Date());
      (a = s.createElement(o)), (m = s.getElementsByTagName(o)[0]);
      a.async = 1;
      a.src = g;
      m.parentNode.insertBefore(a, m);
    })(
      window,
      document,
      "script",
      "//www.google-analytics.com/analytics.js",
      "ga"
    );
    ga("create", "UA-62477704-1", "auto");
    ga("send", "pageview");
  </script>
</html>
